<div class="container p-4">
    <div class="m-1">
        <a class="btn btn-outline-success btn-block" href = "addWorkType" >Agregar un tipo de trabajo</a>
    </div>
    
    <div class="row">
        {{#each work_type}}
        <div class="col-md-3">
            <div class="shadow card text-center">
                <div class="card-body">
                    <p class="p-3 mb-2 bg-success text-white text-uppercase font-weight-light">ID: {{workTypeCode}}</p>
                    <p class="text-muted">Descripcion: {{descriptionWorkType}}</p>
                    
                    <a class="btn btn-success" href="editBranchOffice/{{branchCode}}">
                        Editar
                    </a>

                    <a class="btn btn-danger" href="deleteBranchOffice/{{branchCode}}">
                        Eliminar
                    </a>

                </div>
            </div>
        </div>
        
        {{/each}}
    </div>
</div>